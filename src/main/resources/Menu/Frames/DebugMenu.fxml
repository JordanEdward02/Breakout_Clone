<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>


<DialogPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.Controllers.DebugMenuController">
   <content>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button layoutX="116.0" layoutY="75.0" mnemonicParsing="false" onAction="#LevelSkipButton" text="Skip Level" />
            <Button layoutX="394.0" layoutY="75.0" mnemonicParsing="false" onAction="#ResetBalls" text="Reset Balls" />
            <Slider layoutX="230.0" layoutY="200.0" max="4.0" min="-4.0" fx:id="m_XSlider" onDragDropped="#SetXSpeed" />
            <Slider layoutX="230.0" layoutY="333.0" max="4.0" min="-4.0" fx:id="m_YSlider" onDragDropped="#SetYSpeed" />
            <Label layoutX="278.0" layoutY="154.0" text="X Speed" />
            <Label layoutX="278.0" layoutY="286.0" text="Y Speed" />
         </children>
      </AnchorPane>
   </content>
</DialogPane>
