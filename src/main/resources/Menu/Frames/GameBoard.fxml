<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.Controllers.MainGameController">
   <children>
      <Canvas fx:id="m_GameBoard" height="400.0" width="600.0" />
      <AnchorPane fx:id="m_PauseMenuPane" prefHeight="400.0" prefWidth="600.0" visible="false">
         <children>
            <Button layoutX="259.0" layoutY="174.0" mnemonicParsing="false" onAction="#GameContinue" text="Continue" />
            <Button layoutX="264.0" layoutY="235.0" mnemonicParsing="false" onAction="#GameRestart" text="Restart" />
            <Button layoutX="273.0" layoutY="298.0" mnemonicParsing="false" onAction="#GameExit" text="Exit" />
            <Label layoutX="275.0" layoutY="92.0" text="Pause" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
